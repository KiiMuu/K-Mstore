<ul class="uk-pagination uk-flex-center">
    <li><a href="?page=<%= previousPage %>"><span uk-pagination-previous></span></a></li>
    <% if (currentPage !== 1 && previousPage !== 1) { %>
        <li><a href="?page=1">1</a></li>
    <% } %>
    <% if (hasPreviousPage) { %>
        <li><a href="?page=<%= previousPage %>"><%= previousPage %></a></li>
    <% } %>
    <li><a href="?page=<%= currentPage %>"><%= currentPage %></a></li>
    <% if (hasNextPage) { %>
        <li><a href="?page=<%= nextPage %>"><%= nextPage %></a></li>
    <% } %>
    <% if (lastPage !== currentPage && nextPage !== lastPage) { %>
        <li><a href="?page=<%= lastPage %>"><%= lastPage %></a></li>
    <% } %>
    <li><a href="?page=<%= nextPage %>"><span uk-pagination-next></span></a></li>
</ul>